<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>User List</h1>
    <table>
        <thead>
            <th>Name</th>
            <th>Email</th>
            <th>Password</th>
        </thead>
        <tbody>
            <% for(var i=0;i<users.length;i++){ %>

            <tr>
                <td><a href="/users/userdetail/<%= users[i]._id%>"><%= users[i].name %></a></td>
                <td><%= users[i].email %></td>
                <td><%= users[i].password %></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>